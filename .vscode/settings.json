{
  "python.terminal.activateEnvironment": false,
  "python.formatting.yapfPath": "~/.local/bin/yapf",
  "material-icon-theme.files.associations": {
    "*.test.py": "test-js"
  },
  "explorerExclude.backup": {},
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true,
    "**/node_modules": true,
    "**/venv": true,
    "**/.pytest_cache": true,
    "**/__init__.py": true
  }
}
